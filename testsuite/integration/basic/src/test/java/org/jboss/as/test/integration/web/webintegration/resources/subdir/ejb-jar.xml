<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <description>
       Test for relative ejb-link elements of the form "path/to/file.jar#MyBean"
   </description>
    <enterprise-beans>
        <session>
            <description>Simple Sample Session Bean</description>
            <ejb-name>SessionBean</ejb-name>
            <home>org.jboss.as.test.integration.web.webintegration.interfaces.StatelessSessionHome</home>
            <remote>org.jboss.as.test.integration.web.webintegration.interfaces.StatelessSession</remote>
            <local-home>org.jboss.as.test.integration.web.webintegration.interfaces.StatelessSessionLocalHome</local-home>
            <local>org.jboss.as.test.integration.web.webintegration.interfaces.StatelessSessionLocal</local>
            <ejb-class>org.jboss.as.test.integration.web.webintegration.interfaces.StatelessSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>